<div class="row">
  <div class="col-md-4 col-md-offset-8">
    <div class="login">
      <%= form_for :session, url: admin_path do |f| %>
        <%= f.text_field :email, placeholder: "Email" %>
        <%= f.password_field :password, placeholder: "Password" %>
        <%= f.submit "Login" %>
      <% end %>
    </div>
  </div>
</div>
